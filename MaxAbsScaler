#MAXABS SCALER
from sklearn.preprocessing import MaxAbsScaler
import numpy as np
data=np.array([[10],[20],[30],[40]])
scaler=MaxAbsScaler()
maxabs_data = scaler.fit_transform(data)
print(maxabs_data)

